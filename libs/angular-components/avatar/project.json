{
  "name": "avatar",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/angular-components/avatar/src",
  "prefix": "finastra",
  "tags": [],
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/angular-components/avatar/src/**/*.ts",
          "libs/angular-components/avatar/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/angular-components/avatar/jest.config.ts",
        "passWithNoTests": true,
        "tsConfig": "libs/angular-components/avatar/tsconfig.spec.json"
      },
      "outputs": [
        "{workspaceRoot}/coverage/libs/angular-components/avatar"
      ]
    }
  }
}