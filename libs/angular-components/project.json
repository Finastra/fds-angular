{
  "name": "angular-components",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/angular-components/",
  "prefix": "finastra",
  "tags": [],
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "project": "libs/angular-components/ng-package.json",
        "tsConfig": "libs/angular-components/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/angular-components/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/angular-components/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nrwl/linter:lint",
      "options": {
        "linter": "eslint",
        "config": "libs/angular-components/.eslintrc.json",
        "tsConfig": [
          "libs/angular-components/tsconfig.lib.json",
          "libs/angular-components/tsconfig.spec.json"
        ],
        "exclude": [
          "**/node_modules/**",
          "!libs/angular-components/**/*"
        ]
      }
    }
  }
}