{
  "name": "popover",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/angular-components/popover",
  "prefix": "finastra",
  "tags": [],
  "generators": {
    "@nrwl/angular:component": {
      "styleext": "scss"
    }
  },
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/angular-components/popover/src/**/*.ts",
          "libs/angular-components/popover/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/angular-components/popover/jest.config.ts",
        "tsConfig": "libs/angular-components/popover/tsconfig.spec.json"
      },
      "outputs": [
        "{workspaceRoot}/coverage/libs/angular-components/popover"
      ]
    }
  }
}