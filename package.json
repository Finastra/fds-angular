{
  "name": "@finastra/fds-angular",
  "version": "3.11.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/finastra/fds-angular.git"
  },
  "license": "MIT",
  "workspaces": [
    "themes/angular-theme"
  ],
  "scripts": {
    "add:component": "nx workspace-schematic angular-component",
    "postadd:component": "nx format:write",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "postaffected:test": "node ./scripts/jest-join-coverage.js",
    "build": "ng build",
    "build:devkit": "tsc -p ./tools/devkit/tsconfig.devkit.json && cpx -v \"./tools/devkit/**/*.json\" dist/tools/devkit",
    "build:theme": "ng build angular-theme --prod",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "changelog:components": "cd libs/angular-components && npx standard-version",
    "changelog:theme": "cd themes/angular-theme && npx standard-version",
    "clean": "rimraf dist coverage junit.xml",
    "dep-graph": "nx dep-graph",
    "dev": "npm start -- -o",
    "e2e": "ng e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "node ./scripts/skip.js || ngcc --properties es2020 browser module main && node ./decorate-angular-cli.js",
    "lint": "nx run-many --target=lint --all --exclude=angular-components",
    "ng": "nx",
    "nx": "nx",
    "prerelease": "node scripts/pre-angular-release.js",
    "release": "npx standard-version -an",
    "start": "ng serve",
    "pretest": "rimraf ./coverage ./testresults",
    "test": "ng test",
    "posttest": "node ./scripts/jest-join-coverage.js",
    "testjest": "jest",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "add": "npm install --package-lock-only --no-package-lock --prefix",
    "postadd": "npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.*": [
      "nx format:write --uncommited"
    ],
    "{apps,libs,tools,themes}/**/*.{ts,json,scss,html}": [
      "nx affected:lint --uncommitted --parallel --fix=true"
    ]
  },
  "devDependencies": {
    "@actions/core": "^1.6.0",
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-devkit/core": "^14.1.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.2",
    "@angular-eslint/eslint-plugin": "^14.0.2",
    "@angular-eslint/template-parser": "^14.0.2",
    "@angular/animations": "^14.1.0",
    "@angular/cdk-experimental": "^14.1.0",
    "@angular/cdk": "^14.1.0",
    "@angular/cli": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.0",
    "@angular/language-service": "^14.1.0",
    "@angular/material": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@nrwl/angular": "14.5.1",
    "@nrwl/cli": "14.5.1",
    "@nrwl/cypress": "14.5.1",
    "@nrwl/devkit": "14.5.1",
    "@nrwl/eslint-plugin-nx": "14.5.1",
    "@nrwl/jest": "14.5.1",
    "@nrwl/nx-cloud": "14.2.0",
    "@nrwl/workspace": "14.5.1",
    "@types/command-line-args": "^5.0.0",
    "@types/d3": "6.7.0",
    "@types/fs-extra": "^9.0.11",
    "@types/git-config-path": "^2.0.1",
    "@types/jest": "27.4.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "15.6.1",
    "@types/parse-git-config": "3.0.1",
    "@types/sass": "^1.16.0",
    "@types/smoothscroll-polyfill": "0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "acorn": "^8.2.4",
    "angular-plotly.js": "^4.0.0",
    "autoprefixer": "^10.2.6",
    "command-line-args": "^5.1.1",
    "commander": "^7.2.0",
    "concurrently": "^6.2.1",
    "core-js": "3.15.2",
    "cpx": "^1.5.0",
    "cypress-image-snapshot": "^4.0.1",
    "cypress": "^4.12.1",
    "d3": "6.7.0",
    "dotenv": "10.0.0",
    "elasticlunr": "git://github.com/weixsong/elasticlunr.js.git#f773890392760e0b3c68f0e65dbacb396dff297f",
    "es-abstract": "1.18.3",
    "eslint-config-prettier": "8.3.0",
    "eslint": "7.28.0",
    "fs-extra": "^10.0.0",
    "git-config-path": "^2.0.0",
    "globby": "^11.1.0",
    "husky": "^6.0.0",
    "istanbul-api": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest-canvas-mock": "2.3.1",
    "jest-junit": "13.0.0",
    "jest-preset-angular": "11.1.2",
    "jest": "27.5.1",
    "js-yaml": "^4.1.0",
    "jsonc-parser": "^3.0.0",
    "koa-ejs": "4.3.0",
    "lint-staged": "^11.0.0",
    "lit-html": "2.0.2",
    "lodash-es": "^4.17.21",
    "ng-packagr": "^14.1.0",
    "ngx-color": "^7.0.0",
    "node-sass-import": "^2.0.1",
    "node-watch": "0.7.1",
    "nx": "14.5.1",
    "ora": "^5.4.1",
    "parse-git-config": "^3.0.0",
    "plotly.js": "^1.58.4",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "postcss": "^8.3.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "sass": "^1.44.0",
    "semver": "^7.3.5",
    "sinon": "^12.0.1",
    "smoothscroll-polyfill": "^0.4.4",
    "standard-version": "^9.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-formatter-pretty": "^1.1.2",
    "stylelint-scss": "^3.19.0",
    "stylelint": "^13.13.1",
    "ts-jest": "27.1.4",
    "ts-md5": "1.2.8",
    "ts-node": "10.8.2",
    "tslib": "^2.4.0",
    "typescript": "4.7.4",
    "zone.js": "0.11.7"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  }
}
