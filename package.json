{
  "name": "@finastra/fds-angular",
  "version": "5.0.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/finastra/fds-angular.git"
  },
  "license": "MIT",
  "workspaces": [
    "libs/angular-theme"
  ],
  "scripts": {
    "add:component": "nx workspace-schematic angular-component",
    "postadd:component": "nx format:write",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "build:devkit": "tsc -p ./tools/devkit/tsconfig.devkit.json && npx copyfiles -u 2 \"./tools/devkit/**/*.{json,js,cjs}\" dist/tools/devkit",
    "build:theme": "nx build angular-theme --prod && cd libs/angular-theme/prebuilt-theme",
    "pack:theme": "npm run build:theme && npm pack",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "pack:components": "nx run angular-components:build:production && cd dist/libs/angular-components && npm pack",
    "changelog:components": "cd libs/angular-components && npx standard-version",
    "changelog:theme": "cd libs/angular-theme && npx standard-version",
    "clean": "rimraf dist coverage junit.xml",
    "dep-graph": "nx dep-graph",
    "dev": "npm start -- -o",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "lint": "nx run-many --target=lint --all --exclude=angular-components",
    "ng": "nx",
    "nx": "nx",
    "prerelease": "node scripts/pre-angular-release.js",
    "release": "npx standard-version -an",
    "start": "nx serve",
    "pretest": "rimraf ./coverage ./testresults",
    "test": "jest",
    "test:cov": "nx run-many --target=test --all --parallel --maxWorkers=1",
    "pretest:cov": "rimraf ./coverage ./testresults",
    "posttest:cov": "node ./scripts/jest-join-coverage.js",
    "update": "nx migrate latest",
    "update:check": "nx update",
    "add": "npm install --package-lock-only --no-package-lock --prefix",
    "postadd": "npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.*": [
      "nx format:write --uncommited"
    ],
    "{apps,libs,tools,themes}/**/*.{ts,json,scss,html}": [
      "nx affected:lint --uncommitted --parallel --fix=true"
    ]
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
